package step9;

import java.util.Arrays;
import java.util.Scanner;

public class _1181 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		 
		int n = in.nextInt();

		char[] arr = new char[n];
		 
		 for(int i=0; i<n; i++) {
			 arr = in.nextLine().toCharArray();
		 }
		 
		 Arrays.sort(arr);
		 
		 System.out.println(Arrays.toString(arr));
	}

}
